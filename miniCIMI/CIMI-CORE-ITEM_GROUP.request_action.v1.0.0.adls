definition
	ITEM_GROUP[id1.1.1] matches { -- Request action
		item matches {
			ELEMENT[id2.1.1] matches { -- Action type
				value matches {
					CODED_TEXT[id2.16.1]
				}
			}
			ELEMENT[id2.2.1] matches { -- Request identifier
				value matches {
					IDENTIFIER[id2.17.1]
				}
			}
			ELEMENT[id2.3.1] matches { -- Request datetime
				value matches {
					DATE_TIME[id2.18.1]
				}
			}
			ELEMENT[id2.4.1] matches { -- Request datetime range
				value matches {
					INTERVAL_VALUE<DATE_TIME>[id2.19.1]
				}
			}
			ELEMENT[id2.5.1] matches { -- Request duration
				value matches {
					DURATION[id2.20.1]
				}
			}
			ELEMENT[id2.6.1]  matches { -- Request timing offset
				value matches {
					DURATION[id2.21.1]
				}
			}
			ELEMENT[id2.7.1] matches { -- Request timing offset origin
				value matches {
					DATE_TIME[id2.22.1]
				}
			}
			ELEMENT[id2.9.1] -- Clinical information provided
		}
		participation matches {
			PARTICIPATION[id2.12.1] -- Requester
			PARTICIPATION[id2.12.2] occurrences matches {0..1} -- Location of subject
			PARTICIPATION[id2.12.3] occurrences matches {0..1} -- Location of requester
		}
	}

