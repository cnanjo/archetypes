definition
  Assertion[id1.1.1] matches {
    /key matches {[ac0.1.1]}
    --/precondition[id0.2.1] matches {[ac0.0.2]}
    name matches {
      CODED_TEXT[id0.0.1]
    }
    bodyLocation matches {
      AnatomicalLocation[id0.0.2]
    }
    bodyLocationPrecoord matches {
      CODED_TEXT[id0.0.3]
    }
    causativeAgentCode matches {
      CODED_TEXT[id0.0.4]
    }
    causativeAgent matches {
      use_archetype ClinicalStatement [id0.0.5, CIMI-CORE-ClinicalStatement.clinical_statement.v1] occurrences matches {0..*}
    }
    dueToCode matches {
      CODED_TEXT[id0.0.6]
    }
    dueTo matches {
      use_archetype ClinicalStatement [id0.0.7, CIMI-CORE-ClinicalStatement.clinical_statement.v1] occurrences matches {0..*}
    }
    severity matches {
      CODED_TEXT[id0.0.8]
    }
    clinicalCourse matches {
      CODED_TEXT[id0.0.9]
    }
    interpretation matches {
      Interpretation[id0.0.10]
    }
    periodOfLife matches {
      CODED_TEXT[id0.0.11]
    }
    findingMethod matches {
      CODED_TEXT[id0.0.12]
    }
    ageAtOnset matches {
      QUANTITY[id0.0.13]
    }
    dateAbated matches {
      DATE_TIME[id0.0.14]
    }
    clinicalStatus matches {
      CODED_TEXT[id0.0.15]
    }
    verificationStatus matches {
      CODED_TEXT[id0.0.16]
    }
    diagnosisDate matches {
      DATE_TIME[id0.0.17]
    }
    diseasePhase matches {
      CODED_TEXT[id0.0.18]
    }
    associatedPrecondition matches {
      CODED_TEXT[id0.0.19]
    }
    associatedSignAndSymptom matches {
     CODED_TEXT[id0.0.20]
    }
    alleviatingFactor matches {
     CODED_TEXT[id0.0.21]
    }
    exacerbatingFactor matches {
     CODED_TEXT[id0.0.22]
    }
    periodicity matches {
     CODED_TEXT[id0.0.23]
    }

  }

